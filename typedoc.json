{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "out": "docs",
  "name": "nostr-crypto-utils",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "plugin": ["typedoc-plugin-markdown"],
  "theme": "default",
  "readme": "none",
  "categorizeByGroup": true,
  "categoryOrder": [
    "Key Management",
    "Event Operations",
    "Message Handling",
    "Encryption",
    "Validation",
    "Types",
    "*"
  ],
  "searchInComments": true,
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "navigationLinks": {
    "GitHub": "https://github.com/humanjavaenterprises/nostr-crypto-utils",
    "NPM": "https://www.npmjs.com/package/nostr-crypto-utils"
  },
  "sidebarLinks": {
    "API Reference": "https://humanjavaenterprises.github.io/nostr-crypto-utils/"
  },
  "validation": {
    "invalidLink": true,
    "notDocumented": true
  }
}
